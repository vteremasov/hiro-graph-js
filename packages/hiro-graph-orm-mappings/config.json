{
    "OGIT": "./node_modules/@arago/OGIT/NTO",
    "OUTPUT_DIR": "lib",
    "blacklist": [
        "oslc-arch",
        "oslc-asset",
        "oslc-automation",
        "oslc-change",
        "oslc-core",
        "oslc-crtv",
        "oslc-ems",
        "oslc-perfmon",
        "oslc-qm",
        "oslc-rddl",
        "oslc-reqman"
    ],
    "extras": {
        "ogit/Auth/AccountProfile": {
            "defaultOrg": "/defaultOrg",
            "jobRole": "/jobRole",
            "profilePicture": "/profilePicture",
            "profileSet": "/profileSet"
        }
    },
    "mapping": {
        "ogit/Auth/belongs -> ogit/Auth/Account": "account",
        "ogit/Auth/isMemberOf <- ogit/Auth/Account": "members",
        "ogit/Auth/belongs -> ogit/Auth/Organization": "parentOrg",
        "ogit/Auth/belongs <- ogit/Auth/AccountProfile": "profile",
        "ogit/Auth/isMemberOf -> ogit/Auth/Organization": "orgs",
        "ogit/Auth/isMemberOf -> ogit/Auth/Team": "teams",
        "ogit/belongs -> ogit/Organization": "orgs",
        "ogit/belongs -> ogit/Person": "person",
        "ogit/belongs <- ogit/Person": "members",
        "ogit/owns -> ogit/Certificate": "certificates",
        "ogit/owns -> ogit/Asset": "assets",
        "ogit/complies -> ogit/Person": "owner",
        "ogit/complies -> ogit/Role": "roles",
        "ogit/connects -> ogit/Hyperlink": "links",
        "ogit/connects -> ogit/Person": "owner",
        "ogit/connects <- ogit/Account": "accounts",
        "ogit/connects <- ogit/Comment": "comments",
        "ogit/connects <- ogit/Email": "emails",
        "ogit/connects <- ogit/Person": "owner",
        "ogit/defines <- ogit/Organization": "org",
        "ogit/leads -> ogit/Organization": "leads",
        "ogit/leads <- ogit/Person|ogit/Organization": "leader",
        "ogit/manages -> ogit/Organization": "manages",
        "ogit/manages <- ogit/Person": "admin",
        "ogit/precedes -> ogit/License": "previous",
        "ogit/precedes <- ogit/License": "next",
        "ogit/relates -> ogit/Organization": "related",
        "ogit/reports -> ogit/Person": "managers",
        "ogit/reports <- ogit/Person": "subordinates",
        "ogit/requests -> ogit/LicenseRequest": "licenseRequests",
        "ogit/sends <- ogit/Software/Application": "sends",
        "ogit/UserMeta/loses <- ogit/Person": "loses",
        "ogit/UserMeta/plays <- ogit/Person": "plays",
        "ogit/UserMeta/wins <- ogit/Person": "wins",
        "ogit/uses -> ogit/License": "licenses",
        "ogit/uses <- ogit/Organization": "orgs",
        "ogit/uses <- ogit/Person": "people",
        "ogit/uses <- ogit/Person|ogit/Organization": "users",
        "ogit/versions <- ogit/Auth/Application": "parentVersion",
        "ogit/versions -> ogit/Auth/Application": "appVersion",
        "ogit/consistsOf <- ogit/MRP/Part": "item",
        "ogit/consistsOf -> ogit/MRP/Part": "parts"
    }
}
